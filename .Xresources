! Xft settings:
Xft.dpi:                    98
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                1
Xft.hintstyle:              hintslight

!-------------------------------Xcursor----------------------------------------
Xcursor.theme:		    Adwaita
Xcursor.size:		    32
!--------------------------------urxvt-----------------------------------------
! Size
URxvt.depth:                32
URxvt.geometry:             100x24
URxvt.transparent:          false
URxvt.fading:               0
URxvt.loginShell:           true
URxvt.saveLines:            1024
URxvt.internalBorder:       3
URxvt.lineSpace:            1
URxvt.letterSpace:          0



! Font config
! latin chars:  Inconsolata-g
! Chinese chars:wqy-microhei
!URxvt.font:xft:WenQuanYi Microhei Mono:pixelsize=30:antialias=true
URxvt.font:xft:WenQuanYi Microhei Mono:pixelsize=18:antialias=true
! Clickable URLs
URxvt.perl-ext-common:      default,matcher,clipboard
URxvt.url-launcher:         /usr/bin/firefox
URxvt.matcher.button:       1
URxvt.colorUL:              #4682B4

! Fullscreen: I dont need specific FullScreen function, awesome WM did that for me 







